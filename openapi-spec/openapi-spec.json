                }
              }
            },
            "description" : "Invalid vehicle_id for User"
          },
          "default": {
            "description": "Default",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "maxLength": 1
                }
              }
            }
          }
        }
      },
      "parameters" : [ {
        "in" : "path",
        "name" : "vehicleId",
        "required" : true,
        "schema" : {
          "type": "string",
          "format": "uuid",
          "example": "0be319f0-f0dd-44aa-af0b-af927f3a383f",
          "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
          "minLength": 36,
          "maxLength": 36,
          "x-42c-format": "o:uuid"
        }
      } ]
    }
  },
