  "paths" : {
    "/identity/api/auth/signup" : {
      "post" : {
        "operationId" : "signup",
        "summary" : "Sign up",
        "description" : "Used to create an account",
        "tags" : [ "Identity / Auth" ],
        "security" : [ ],
        "x-42c-no-authentication": true,
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "type" : "object",
                "required" : [ "email", "name", "number", "password" ],
                "additionalProperties": false,
                "properties" : {
                  "email" : {
                    "type": "string",
                    "pattern": "^(?:[\\w\\-+!#$%&'*/=?^`|{}~]+(?:\\.[\\w\\-+!#$%&'*/=?^`|{}~]+)*)@(?:(?:[A-Za-z0-9](?:[\\w\\-]{0,61}[A-Za-z0-9])?)(?:\\.(?:[A-Za-z0-9](?:[\\w\\-]{0,61}[A-Za-z0-9])?))*\\.[A-Za-z]{2,})$",
                    "minLength": 6,
                    "maxLength": 320,
                    "format": "o:Vehicle-Locator-API:email",
                    "x-42c-format": "o:Vehicle-Locator-API:email",
                    "example": "scanuser@test.com"
                  },
                  "name" : {
                    "type" : "string",
                    "pattern": "^.{3,300}$",
                    "minLength": 3,
                    "maxLength": 300,
                    "example": "Conf Scan User"
                  },
                  "number" : {
                    "type" : "string",
                    "pattern": "^[0-9]{10,10}$",
                    "minLength": 10,
                    "maxLength": 10
                  },
                  "password" : {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9&@#!?]{3,300}$",
                    "minLength": 3,
                    "maxLength": 300,
                    "format": "o:Vehicle-Locator-API:password",
                    "example": "scanpassword",
                    "x-42c-format": "o:Vehicle-Locator-API:password"
                  }
                }
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/DefaultResponse"
                }
              }
            },
            "description" : "User successfully registered"
          },
          "403" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/DefaultResponse"
                }
              }
            },
            "description" : ""
          },
          "500" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/DefaultResponse"
                }
              }
            },
            "description" : ""
          },
          "default": {
            "description": "Default",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "maxLength": 1
                }
              }
            }
          }
        }
      },
      "parameters" : [ ]
    },
