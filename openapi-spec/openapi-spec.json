    "/identity/api/v2/vehicle/vehicles" : {
      "get" : {
        "operationId" : "get_vehicles",
        "summary" : "Get user vehicles",
        "tags" : [ "Identity / Vehicle" ],
        "security" : [ {
          "bearerAuth" : [ ]
        } ],
        "responses" : {
          "200" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "type" : "object",
                    "required" : [ "id", "model", "owner", "pincode", "status", "uuid", "vehicleLocation", "vin", "year" ],
                    "additionalProperties": false,
                    "properties" : {
                      "id" : {
                        "type" : "number"
                      },
                      "uuid" : {
                        "type": "string",
                        "format": "uuid",
                        "example": "a83a29f5-0d63-46f2-8f2e-44c2f1d2e07e",
                        "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$",
                        "minLength": 36,
                        "maxLength": 36,
                        "x-42c-format": "o:uuid"
                      },
                      "year" : {
                        "type" : "number"
                      },
                      "status" : {
                        "type" : "string"
                      },
                      "vin" : {
                        "type": "string",
                        "format": "o:Vehicle-Locator-API:VIN",
                        "example": "SALVA2AE4EH877322",
                        "pattern": "^[A-Z0-9]{17}$",
                        "minLength": 17,
                        "maxLength": 17,
                        "x-42c-format": "o:Vehicle-Locator-API:VIN"
                      },
                      "pincode" : {
                        "type": "string",
                        "format": "o:Vehicle-Locator-API:pincode",
                        "example": "1039",
                        "pattern": "^[0-9]{4}$",
                        "minLength": 4,
                        "maxLength": 4,
                        "x-42c-format": "o:Vehicle-Locator-API:pincode"
                      },
                      "owner" : {
                        "$ref" : "#/components/schemas/User"
                      },
                      "model" : {
                        "type" : "object",
                        "required" : [ "fuel_type", "id", "model", "vehicle_img", "vehiclecompany" ],
                        "additionalProperties": false,
                        "properties" : {
                          "model" : {
                            "type" : "string"
                          },
                          "fuel_type" : {
                            "type" : "string"
                          },
                          "vehicle_img" : {
                            "type" : "string"
                          },
                          "id" : {
                            "type" : "number"
                          },
                          "vehiclecompany" : {
                            "type" : "object",
                            "required" : [ "id", "name" ],
                            "additionalProperties": false,
                            "properties" : {
                              "id" : {
                                "type" : "number"
                              },
                              "name" : {
                                "type" : "string"
                              }
                            }
                          }
                        }
                      },
                      "vehicleLocation" : {
                        "type" : "object",
                        "required" : [ "id", "latitude", "longitude" ],
                        "additionalProperties": false,
                        "properties" : {
                          "id" : {
                            "type" : "number"
                          },
                          "latitude" : {
                            "type" : "string"
                          },
                          "longitude" : {
                            "type" : "string"
                          }
                        }
                      }
                    }
                  },
                  "maxItems": 10
                }
              }
            },
            "description" : ""
          },
          "500" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/DefaultResponse"
                }
              }
            },
            "description" : "Internal Server Error"
          },
          "default": {
            "description": "Default",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "maxLength": 1
                }
              }
            }
          }
        }
      },
      "parameters" : [ ]
    },
