{
  "version": "2.0.0",
  "runtimeConfiguration": {
    "environment": "default",
    "laxTestingModeEnabled": false,
    "apiConnectivityCheck": true,
    "logLevel": "error",
    "logDestination": "stdout+platform",
    "logMaxFileSize": 2147483648,
    "requestHeaderNameRequestId": "x-scan-request-id",
    "requestHeaderNameScenarioId": "x-scan-scenario-id",
    "requestHeaderNameRequestType": "x-scan-request-type",
    "requestFlowrate": 100,
    "requestTimeout": 30,
    "requestTlsInsecureSkipVerify": true,
    "responseFollowRedirection": false,
    "responseMaxBodySizeScan": 10485760,
    "happyPathOnly": false,
    "maxRequestRetryAttempts": 5,
    "maxScanDuration": 1800,
    "memoryLimit": 2147483648,
    "memoryTimeSpan": 10,
    "reportMaxRequestSizeHappyPath": 8092,
    "reportMaxRequestSizeTest": 8092,
    "reportIncludeRequestBody": true,
    "reportIncludeResponseBody": true,
    "reportMaxHttpResponseSizeHappyPath": 8092,
    "reportMaxBodySizeHappyPath": 8092,
    "reportMaxHttpResponseSizeTest": 8092,
    "reportMaxBodySizeTest": 8092,
    "reportIssuesOnly": false,
    "reportMaxIssues": 1000,
    "reportMaxSize": 20971520,
    "reportGenerateCurlCommand": true
  },
  "customizations": {
    "happyPaths": {
      "retry": 1,
      "responsePolicy": {
        "httpStatusExpected": true,
        "mustBeConformant": true
      },
      "httpStatusExpected": []
    },
    "tests": {
      "responsePolicy": {
        "httpStatusExpected": true,
        "mustBeConformant": true
      }
    }
  },
  "environments": {
    "default": {
      "variables": {
        "bearerAuth": {
          "name": "SCAN42C_SECURITY_BEARERAUTH",
          "from": "environment",
          "required": false
        },
        "host": {
          "name": "SCAN42C_HOST",
          "from": "environment",
          "required": false,
          "default": "http://localhost:8888"
        }
      }
    }
  },
  "operations": {
    "add_vehicle": {
      "operationId": "add_vehicle",
      "request": {
        "operationId": "add_vehicle",
        "auth": [
          "bearerAuth"
        ],
        "request": {
          "type": "42c",
          "details": {
            "operationId": "add_vehicle",
            "method": "POST",
            "url": "{{host}}/identity/api/v2/vehicle/add_vehicle",
            "headers": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "requestBody": {
              "mode": "json",
              "json": {
                "pincode": "{{$randomFromSchema}}",
                "vin": "{{$randomFromSchema}}"
              }
            }
          }
        },
        "defaultResponse": "200",
        "responses": {
          "200": {
            "expectations": {
              "httpStatus": 200
            }
          },
          "403": {
            "expectations": {
              "httpStatus": 403
            }
          }
        }
      },
      "scenarios": [
        {
          "key": "happy.path",
          "requests": [
            {
              "fuzzing": true,
              "$ref": "#/operations/add_vehicle/request"
            }
          ],
          "fuzzing": true
        }
      ]
    },
    "get_location": {
      "operationId": "get_location",
      "request": {
        "operationId": "get_location",
        "auth": [
          "bearerAuth"
        ],
        "request": {
          "type": "42c",
          "details": {
            "operationId": "get_location",
            "method": "GET",
            "url": "{{host}}/identity/api/v2/vehicle/{vehicleId}/location",
            "paths": [
              {
                "key": "vehicleId",
                "value": "{{vehicleId}}"
              }
            ]
          }
        },
        "defaultResponse": "200",
        "responses": {
          "200": {
            "expectations": {
              "httpStatus": 200
            }
          },
          "404": {
            "expectations": {
              "httpStatus": 404
            }
          },
          "default": {
            "expectations": {
              "httpStatus": "default"
            }
          }
        }
      },
      "before": [
        {
          "$ref": "#/operations/get_vehicles/request",
          "responses": {
            "200": {
              "expectations": {
                "httpStatus": 200
              },
              "variableAssignments": {
                "vehicleId": {
                  "in": "body",
                  "from": "response",
                  "contentType": "json",
                  "path": {
                    "type": "jsonPointer",
                    "value": "/0/uuid"
                  }
                }
              }
            }
          }
        }
      ],
      "scenarios": [
        {
          "key": "happy.path",
          "requests": [
            {
              "fuzzing": true,
              "$ref": "#/operations/get_location/request"
            }
          ],
          "fuzzing": true
        }
      ]
    },
    "get_vehicles": {
      "operationId": "get_vehicles",
      "request": {
        "operationId": "get_vehicles",
        "auth": [
          "bearerAuth"
        ],
        "request": {
          "type": "42c",
          "details": {
            "operationId": "get_vehicles",
            "method": "GET",
            "url": "{{host}}/identity/api/v2/vehicle/vehicles"
          }
        },
        "defaultResponse": "200",
        "responses": {
          "200": {
            "expectations": {
              "httpStatus": 200
            }
          },
          "500": {
            "expectations": {
              "httpStatus": 500
            }
          },
          "default": {
            "expectations": {
              "httpStatus": "default"
            }
          }
        }
      },
      "scenarios": [
        {
          "key": "happy.path",
          "requests": [
            {
              "fuzzing": true,
              "$ref": "#/operations/get_vehicles/request"
            }
          ],
          "fuzzing": true
        }
      ]
    },
    "login": {
      "operationId": "login",
      "request": {
        "operationId": "login",
        "request": {
          "type": "42c",
          "details": {
            "operationId": "login",
            "method": "POST",
            "url": "{{host}}/identity/api/auth/login",
            "headers": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "requestBody": {
              "mode": "json",
              "json": {
                "email": "scanuser@test.com",
                "password": "scanpassword"
              }
            }
          }
        },
        "defaultResponse": "200",
        "responses": {
          "200": {
            "expectations": {
              "httpStatus": 200
            },
            "variableAssignments": {
              "bearerAuth": {
                "in": "body",
                "from": "response",
                "contentType": "json",
                "path": {
                  "type": "jsonPointer",
                  "value": "/token"
                }
              }
            }
          },
          "500": {
            "expectations": {
              "httpStatus": 500
            }
          },
          "default": {
            "expectations": {
              "httpStatus": "default"
            }
          }
        }
      },
      "scenarios": [
        {
          "key": "happy.path",
          "requests": [
            {
              "fuzzing": true,
              "$ref": "#/operations/login/request"
            }
          ],
          "fuzzing": true
        }
      ]
    },
    "signup": {
      "operationId": "signup",
      "request": {
        "operationId": "signup",
        "request": {
          "type": "42c",
          "details": {
            "operationId": "signup",
            "method": "POST",
            "url": "{{host}}/identity/api/auth/signup",
            "headers": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "requestBody": {
              "mode": "json",
              "json": {
                "email": "{{email}}",
                "name": "Conf Scan User",
                "number": "{{$randomFromSchema}}",
                "password": "scanpassword"
              }
            }
          }
        },
        "defaultResponse": "200",
        "responses": {
          "200": {
            "expectations": {
              "httpStatus": 200
            }
          },
          "403": {
            "expectations": {
              "httpStatus": 403
            }
          },
          "500": {
            "expectations": {
              "httpStatus": 500
            }
          },
          "default": {
            "expectations": {
              "httpStatus": "default"
            }
          }
        }
      },
      "scenarios": [
        {
          "key": "happy.path",
          "requests": [
            {
              "fuzzing": true,
              "$ref": "#/operations/signup/request",
              "environment": {
                "email": "{{$randomString}}@test.com"
              }
            }
          ],
          "fuzzing": true
        }
      ]
    }
  },
  "authenticationDetails": [
    {
      "bearerAuth": {
        "type": "bearer",
        "default": "bearerAuth",
        "credentials": {
          "bearerAuth": {
            "credential": "{{bearerAuth}}",
            "description": "bearerAuth security",
            "requests": [
              {
                "$ref": "#/operations/login/request"
              }
            ]
          }
        }
      }
    }
  ]
}